{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "vercel-node@12"
        }
    },
    "installConfig": {
        "include": {
            "force": true
        }
    }
}