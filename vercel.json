{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "env": {
        "CI": "1"
    },
    "install": {
        "env": {
            "CI": "1"
        },
        "ignoreScripts": true
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}